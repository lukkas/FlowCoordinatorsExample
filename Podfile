# platform :ios, '9.0'

target 'Type' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # RX
  pod 'RxSwift', :git => 'https://github.com/ReactiveX/RxSwift.git', :branch => 'swift-3.0'
  pod 'RxCocoa', :git => 'https://github.com/ReactiveX/RxSwift.git', :branch => 'swift-3.0'

  target 'TypeTests' do
    inherit! :search_paths
    pod 'RxTests', :git => 'https://github.com/ReactiveX/RxSwift.git', :branch => 'swift-3.0'
  end

  target 'TypeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
